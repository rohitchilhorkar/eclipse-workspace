package Product;

public class Toys extends Product{	
	
	public Toys() {
		
	}
	

	public Toys(String productName, int productId, int inStock, double productPrice, String productDescription) {
		Product data = new Product(productName, productId, "Toys",inStock, productPrice, productDescription);
		product[index++] = data;		
	}
	
	void display(String productName, int productId, int inStock, double productPrice,
			String productDescription) {
	
	}
	
}
